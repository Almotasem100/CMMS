  <div class="content">
    <div class="content_resize">
      <div id="rightarea" class="limiter">
          <div class="container-table100">
            <div class="wrap-table100">
              <div class="table100">
                <table id="hakona">
                  <thead>
                    <tr class="table100-head">
                      <th class="column3">Name</th>
                      <th class="column3">ID</th>
                      <th class="column4">E-mail</th>
                      <th class="column4">Mobile-Number</th>
                      <th class="column4">Role</th>
                      <th class="column3">Position</th>
                      <th class="column4">Department</th>
                      <th class="column3">Qualifications</th>
                      <th class="column4">Salary</th>
                    </tr>
                  </thead>

                    <tbody>
                    {{#each employee}}
                      <tr>
                        <td class="column3">{{name}}</td>
                        <td class="column3">{{id}}</td>
                        <td class="column4">{{email}}</td>
                        <td class="column4">{{mobile_no}}</td>
                        <td class="column4">{{role}}</td>
                        <td class="column3">{{position}}</td>
                        <th class="column4">{{department}}</th>
                        <th class="column3">{{qualifications}}</th>
                        <th class="column4">{{salary}}</th>
                      </tr>
                      {{/each}}
                    </tbody>

                </table>
              </div>
            </div>
        </div>
      </div>
      <div id="leftarea" class="sidebar">
        <div class="searchform">
          <form id="formsearch" name="formsearch" method="post" action="/Emp_table">
            <span>
            <input name="editbox_search" class="editbox_search" id="editbox_search" maxlength="80" placeholder="Employee ID" type="text" />
            </span>
              <button name="button_search"  class="button_search" type="image" ><img src="images/search.gif"></button>
          </form>
        </div>
        
        <div class="clr"></div>
        <div class="gadget">
          <h2 class="star"><span>Sidebar</span> Menu</h2>
          <div class="clr"></div>
          <ul class="sb_menu">
              <li><a href="/index">Home</a></li>
              <li><a href="/AddEquipment">Add New Equipment</a></li>
              <li><a href="/Report">Report</a></li>
              <li><a href="/Dailypass">Daily Pass</a></li>
              <li><a href="/PPM">PPM</a></li>
              <li><a href="/Calibration">Calibration</a></li>
              <li><a href="/store">Store</a></li>

          </ul>
        </div>
      </div>
      <div class="clr"></div>
    </div>
  </div>
