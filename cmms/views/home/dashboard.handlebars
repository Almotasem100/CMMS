
<link rel="stylesheet" href="./public/css/Style.css">
<div class="content">
    <div class="content_resize">
        <div id="rightarea" class="limiter">
            <div class="container-table100">
                <div class="wrap-table100">
                    <div class="table100">

                        <table id="hakona">
                            <caption>Reports Table</caption>
                            <thead>
                            <tr class="table100-head">
                                <th class="column3">Department</th>
                                <th class="column3">Equipment-Name</th>
                                <th class="column3">Equipment-ID</th>

                                <th class="column4">Manufacturer</th>
                                <th class="column4">Model</th>
                                <th class="column4">Serial-Number</th>
                                <th class="column3">Fault-Date</th>
                                <th class="column4">Fault-Description</th>
                                <th class="column3">Inspection-Date</th>
                            </tr>
                            </thead>
                            <tbody>
                            {{#each ppm}}
                                <tr>
                                    <td class="column3">{{department}}</td>
                                    <td class="column3">{{nomenclature}}</td>
                                    <td class="column4">{{serial_no}}</td>
                                    <td class="column4">{{ppm_id}}</td>
                                    <td class="column4">{{time_period}}</td>
                                    <td class="column3">{{from_date}}</td>
                                    <th class="column4">{{to_date}}</th>
                                    <th class="column3">{{assigned_to}}</th>
                                    <th class="column4">{{ppm_task}}</th>
                                    <th class="column4">{{contract_id}}</th>
                                    <th class="column4">{{status}}</th>
                                </tr>
                            {{/each}}
                            </tbody>
                        </table><br><br><br>
                        <table id="hakona" class="separate">
                            <caption>PPM Table</caption>

                            <thead>

                            <tr class="table100-head">
                                <th class="column3">Department</th>
                                <th class="column3">Nomenclature</th>
                                <th class="column4">Equipment-ID</th>
                                <th class="column4">Serial-Number</th>
                                <th class="column3">Time-Period</th>
                                <th class="column4">From-Date</th>
                                <th class="column3">To-Date</th>
                                <th class="column4">Assigned-to</th>
                                <th class="column4">PPM-Task</th>
                                <th class="column4">Contact-ID</th>
                                <th class="column4">Status</th>

                            </tr>
                            </thead>
                        </table><br><br><br>
                        <table id="hakona" class="separate">
                            <caption>Dailypass Table</caption>
                            <thead>
                            <tr class="table100-head">
                                <th class="column3">Department</th>
                                <th class="column3">Equipment-Name</th>
                                <th class="column3">Equipment-ID</th>


                                <th class="column4">Serial-Number</th>
                                <th class="column3">Technician-Name</th>
                                <th class="column4">Physical-Condition</th>
                                <th class="column3">Batteries-Condition</th>
                                <th class="column3">Cables & Ports-Condition</th>
                                <th class="column3">Self-Test</th>
                                <th class="column3">Inspection-Date</th>
                                <th class="column3">Comment</th>




                            </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div id="leftarea" class="sidebar">
            <div class="searchform">
                <form id="formsearch" name="formsearch" method="post" action="#">
            <span>
            <input name="editbox_search" class="editbox_search" id="editbox_search" maxlength="80" value="Search our ste:" type="text" />
            </span>
                    <input name="button_search" src="images/search.gif" class="button_search" type="image" />
                </form>
            </div>
            <div class="clr"></div>
            <div class="gadget">
                <h2 class="star"><span>Sidebar</span> Menu</h2>
                <div class="clr"></div>
                <ul class="sb_menu">
                    <li><a href="#">Home</a></li>
                    <li><a href="#">TemplateInfo</a></li>
                    <li><a href="#">Style Demo</a></li>
                    <li><a href="#">Blog</a></li>
                    <li><a href="#">Archives</a></li>
                    <li><a href="#">Web Templates</a></li>
                </ul>
            </div>
        </div>
        <div class="clr"></div>
    </div>
</div>


